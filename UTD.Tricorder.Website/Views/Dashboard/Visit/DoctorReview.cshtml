@{
    var entityName = vVisit.EntityName;
    var entity = Framework.Common.EntityFactory.GetEntityInfoByName(entityName, "");
}

@{
    string modelPrefix = "model.SysReview.";
    string sectionName = "SysReview";
}

@{ ViewBag.ViewTitle = "Visit information"; }
@Html.Partial("~/Views/Dashboard/_Layout/_MasterViewHeader.cshtml")

@{ ViewBag.ActiveTab = "DoctorReview";}
@Html.Partial("~/Views/Dashboard/Visit/_VisitDetailsTabs.cshtml")

<form role="form"
      ng-controller="@(entityName)@(sectionName)Controller"
      id="@(entityName)@(sectionName)Frm"
      onsubmit="return false;">

    @*<div class="row">
        <div class="col-md-6">*@


            @* ----------------------------------  General  ------------------------------ *@

            @DoctorReview.GetSectionHeader(modelPrefix, "General", "General")
            @FWHtml.CheckBox(modelPrefix, "GeneralHasSignificantWeightLoss", "Significant weight loss")
            @FWHtml.CheckBox(modelPrefix, "GeneralHasLossOfFeelingOfWellBeing", "Loss of feeling of well-being")
            @FWHtml.CheckBox(modelPrefix, "GeneralHasFatigueOrLossOfEnergy", "Fatigue or loss of energy")
            @FWHtml.CheckBox(modelPrefix, "GeneralHasDifficultySleeping", "Difficulty sleeping")
            @DoctorReview.GetSectionFooter(modelPrefix, "General", "General")

            @* ----------------------------------  Eyes  ------------------------------ *@

            @DoctorReview.GetSectionHeader(modelPrefix, "Eyes", "Eyes")
            @FWHtml.CheckBox(modelPrefix, "EyesHasBlurredVision", "Blurred vision")
            @FWHtml.CheckBox(modelPrefix, "EyesHasDoubleVision", "Double Vision")
            @FWHtml.CheckBox(modelPrefix, "EyesHasSpots", "Spots in front of your eyes")
            @FWHtml.CheckBox(modelPrefix, "EyesHasPain", "Eye pain/irritation")
            @FWHtml.CheckBox(modelPrefix, "EyesHasNeedLens", "Need for corrective lenses")
            @DoctorReview.GetSectionFooter(modelPrefix, "Eyes", "Eyes")

            @* ----------------------------------  Ear-Nose- Throat  ------------------------------ *@

            @DoctorReview.GetSectionHeader(modelPrefix, "EarNose", "Ear-Nose- Throat")
            @FWHtml.CheckBox(modelPrefix, "EarNoseHasChronicHeadaches", "Chronic headaches")
            @FWHtml.CheckBox(modelPrefix, "EarNoseHasHearingLoss", "Hearing loss")
            @FWHtml.CheckBox(modelPrefix, "EarNoseHasRingingInEars", "Ringing in ears")
            @FWHtml.CheckBox(modelPrefix, "EarNoseHasDizziness", "Dizziness")
            @FWHtml.CheckBox(modelPrefix, "EarNoseHasChronicNasalCongestion", "Chronic nasal congestion")
            @FWHtml.CheckBox(modelPrefix, "EarNoseHasNoseBleeds", "Nose bleeds")
            @FWHtml.CheckBox(modelPrefix, "EarNoseHasNoseObstruction", "Nose obstruction")
            @FWHtml.CheckBox(modelPrefix, "EarNoseHasBleedingGums", "Bleeding gums")
            @FWHtml.CheckBox(modelPrefix, "EarNoseHasSoreThroat", "Sore throat")
            @FWHtml.CheckBox(modelPrefix, "EarNoseHasToothache", "Toothache")
            @FWHtml.CheckBox(modelPrefix, "EarNoseHasBreathOdor", "Breath Odor")
            @FWHtml.CheckBox(modelPrefix, "EarNoseHasHoarseness", "Hoarseness")
            @DoctorReview.GetSectionFooter(modelPrefix, "EarNose", "Ear-Nose- Throat")

            @* ----------------------------------  Respiratory  ------------------------------ *@

            @DoctorReview.GetSectionHeader(modelPrefix, "Respiratory", "Respiratory")
            @FWHtml.CheckBox(modelPrefix, "RespiratoryHasShortnessOfBreath", "Shortness of breath")
            @FWHtml.CheckBox(modelPrefix, "RespiratoryHasCough", "Cough")
            @FWHtml.CheckBox(modelPrefix, "RespiratoryHasChestCongestion", "Chest congestion")
            @FWHtml.CheckBox(modelPrefix, "RespiratoryHasWheezing", "Wheezing")
            @FWHtml.CheckBox(modelPrefix, "RespiratoryHasCoughingUpBlood", "Coughing up blood")
            @FWHtml.CheckBox(modelPrefix, "RespiratoryHasChoking", "Choking")
            @FWHtml.CheckBox(modelPrefix, "RespiratoryHasNoisyBreathing", "Noisy breathing")
            @FWHtml.CheckBox(modelPrefix, "RespiratoryHasHistoryOfPneumonia", "History of pneumonia")
            @FWHtml.CheckBox(modelPrefix, "RespiratoryHistoryOfTuberculosis", "History of Tuberculosis (T B)")
            @DoctorReview.GetSectionFooter(modelPrefix, "Respiratory", "Respiratory")

            @* ----------------------------------  Cardiovascular  ------------------------------ *@

            @DoctorReview.GetSectionHeader(modelPrefix, "Cardiovascular", "Cardiovascular")
            @FWHtml.CheckBox(modelPrefix, "CardioHasChestPain", "Chest pain")
            @FWHtml.CheckBox(modelPrefix, "CardioHasHeartFlutteringRacing", "Heart fluttering/racing")
            @FWHtml.CheckBox(modelPrefix, "CardioHasHeartMurmur", "Heart murmur")
            @FWHtml.CheckBox(modelPrefix, "CardioHasDecreasedExerciseTolerance", "Decreased exercise tolerance")
            @FWHtml.CheckBox(modelPrefix, "CardioHasDifficultyBreathingLying", "Difficulty breathing when lying down")
            @FWHtml.CheckBox(modelPrefix, "CardioHasAwakeningShortnessBreath", "Awakening due to shortness of breath")
            @FWHtml.CheckBox(modelPrefix, "CardioHasLegSwelling", "Leg Swelling")
            @FWHtml.CheckBox(modelPrefix, "CardioHasPainButtocksExercise", "Pain in buttocks or legs with exercise")
            @FWHtml.CheckBox(modelPrefix, "CardioHasSensitivityOfhands_feet", "Sensitivity of hands/feet to temperature changes")
            @DoctorReview.GetSectionFooter(modelPrefix, "Cardiovascular", "Cardiovascular")


            @* ----------------------------------  Breast  ------------------------------ *@

            @DoctorReview.GetSectionHeader(modelPrefix, "Breast", "Breast")
            @FWHtml.CheckBox(modelPrefix, "BreastHasBreastLump", "Breast lump")
            @FWHtml.CheckBox(modelPrefix, "BreastHasBreastPain", "Breast pain")
            @FWHtml.CheckBox(modelPrefix, "BreastHasNippleDischarge", "Nipple discharge")
            @DoctorReview.GetSectionFooter(modelPrefix, "Breast", "Breast")

            @* ----------------------------------  Gastrointestinal  ------------------------------ *@

            @DoctorReview.GetSectionHeader(modelPrefix, "Gastrointestinal", "Gastrointestinal")
            @FWHtml.CheckBox(modelPrefix, "GastroHasStomachPain", "Stomach pain")
            @FWHtml.CheckBox(modelPrefix, "GastroHasNausea", "Nausea")
            @FWHtml.CheckBox(modelPrefix, "GastroHasVomiting", "Vomiting")
            @FWHtml.CheckBox(modelPrefix, "GastroHasDiarrhea", "Diarrhea")
            @FWHtml.CheckBox(modelPrefix, "GastroHasConstipation", "Constipation")
            @FWHtml.CheckBox(modelPrefix, "GastroHasFrequentHeartburn", "Frequent heartburn")
            @FWHtml.CheckBox(modelPrefix, "GastroHasIndigestion", "Indigestion")
            @FWHtml.CheckBox(modelPrefix, "GastroHasBelchingSourTaste", "Belching/sour taste")
            @FWHtml.CheckBox(modelPrefix, "GastroHasDifficultySwallowing", "Difficulty swallowing")
            @FWHtml.CheckBox(modelPrefix, "GastroHasBloating", "Bloating")
            @FWHtml.CheckBox(modelPrefix, "GastroHasHistoryOfHepatitis", "History of hepatitis")
            @FWHtml.CheckBox(modelPrefix, "GastroHasHistoryOfYellowJaundice", "History of yellow jaundice")
            Rectal:
            @FWHtml.CheckBox(modelPrefix, "GastroHasRectalBleeding", "Rectal bleeding")
            @FWHtml.CheckBox(modelPrefix, "GastroHasRectalPain", "Rectal pain or irritation")
            @FWHtml.CheckBox(modelPrefix, "GastroHasRectalSwellingHemorrhoids", "Swelling or hemorrhoids")

            @DoctorReview.GetSectionFooter(modelPrefix, "Gastrointestinal", "Gastrointestinal")


        @*</div>
        <div class="col-md-6">*@



            @* ----------------------------------  Genitourinary  ------------------------------ *@

            @DoctorReview.GetSectionHeader(modelPrefix, "Genitourinary", "Genitourinary")
            @FWHtml.CheckBox(modelPrefix, "GenitourinaryHasFrequentUrination", "Frequent urination")
            @FWHtml.CheckBox(modelPrefix, "GenitourinaryHasFrequentUrgeToPee", "Frequent urge to pee")
            @FWHtml.CheckBox(modelPrefix, "GenitourinaryHasPainOnUrination", "Pain on urination")
            @FWHtml.CheckBox(modelPrefix, "GenitourinaryHasBloodyUrine", "Bloody urine")
            @FWHtml.CheckBox(modelPrefix, "GenitourinaryHasDischargeFromPenis", "Discharge from penis")
            @FWHtml.CheckBox(modelPrefix, "GenitourinaryHasTroubleStartingUrination", "Trouble starting urination")
            @FWHtml.CheckBox(modelPrefix, "GenitourinaryHasInterruptionOfUrine", "Interruption of urine stream")
            @FWHtml.CheckBox(modelPrefix, "GenitourinaryHasLossOfBladderControl", "Loss of bladder control")
            @FWHtml.CheckBox(modelPrefix, "GenitourinaryHasDeclineInSexualDesire", "Decline in sexual desire")

            Men:
            @FWHtml.CheckBox(modelPrefix, "GenitourinaryHasDribbling", "Dribbling")
            @FWHtml.CheckBox(modelPrefix, "GenitourinaryHasPainOrSwellingOfPenis", "Pain or swelling of penis")
            @FWHtml.CheckBox(modelPrefix, "GenitourinaryHasPainOrSwellingOfScrotalSac", "Pain or swelling of scrotal sac")
            @FWHtml.CheckBox(modelPrefix, "GenitourinaryHasPainOrSwellingInGroin", "Pain or swelling in groin")
            @FWHtml.CheckBox(modelPrefix, "GenitourinaryHasDifficultyErections", "Difficulty having erections or reaching climax")

            Women:
            @FWHtml.CheckBox(modelPrefix, "GenitourinaryHasFrequentUrinaryInfections", "Frequent urinary infections")
            @FWHtml.CheckBox(modelPrefix, "GenitourinaryHasPressureInVagina", "Pressure in vagina")
            @FWHtml.CheckBox(modelPrefix, "GenitourinaryHasVaginalWallWeakness", "Vaginal wall weakness/protrusion")
            @FWHtml.CheckBox(modelPrefix, "GenitourinaryHasFrequentLossOfUrine", "Frequent loss of urine")
            @FWHtml.CheckBox(modelPrefix, "GenitourinaryHasVaginalDischarge", "Vaginal discharge")
            @FWHtml.CheckBox(modelPrefix, "GenitourinaryHasVaginalIrritation", "Vaginal irritation")
            @FWHtml.CheckBox(modelPrefix, "GenitourinaryHasVaginalDryness", "Vaginal dryness")

            @FWHtml.CheckBox(modelPrefix, "GenitourinaryHasVaginalRedness", "Vaginal redness")
            @FWHtml.CheckBox(modelPrefix, "GenitourinaryHasVaginalPain", "Vaginal pain")
            @FWHtml.CheckBox(modelPrefix, "GenitourinaryHasPainfulIntercourse", "Painful intercourse")
            @FWHtml.CheckBox(modelPrefix, "GenitourinaryHasDifficultyInSexualResponse", "Difficulty in sexual response")
            @FWHtml.CheckBox(modelPrefix, "GenitourinaryHasHotFlashes", "Hot flashes")
            @FWHtml.CheckBox(modelPrefix, "GenitourinaryHasChangeInPeriods", "Change in periods (menstrual flow, frequency)")
            @FWHtml.CheckBox(modelPrefix, "GenitourinaryHasMotherTookDES", "Mother took DES while pregnant with me")
            @FWHtml.CheckBox(modelPrefix, "GenitourinaryHasPainfulPeriods", "Painful periods")
            @FWHtml.CheckBox(modelPrefix, "GenitourinaryHasTroublesomePeriods", "Troublesome symptoms before/during periods")
            @FWHtml.CheckBox(modelPrefix, "GenitourinaryHasOtherPelvicPain", "Other pelvic pain")
            <div>
                Please indicate:
                Number of pregnancies
                <input type="text" class="form-control" name="GenitourinaryPregnanciesCount" placeholder="" data-ng-model="@(modelPrefix + "GenitourinaryPregnanciesCount")" />
                Number of deliveries
                <input type="text" class="form-control" name="GenitourinaryDeliveries" placeholder="" data-ng-model="@(modelPrefix + "GenitourinaryDeliveries")" />
                Number of miscarriages/abortions
                <input type="text" class="form-control" name="GenitourinaryAbortions" placeholder="" data-ng-model="@(modelPrefix + "GenitourinaryAbortions")" />
                Age at onset Of periods
                <input type="text" class="form-control" name="GenitourinaryAgeAtOnsetOfPeriods" placeholder="" data-ng-model="@(modelPrefix + "GenitourinaryAgeAtOnsetOfPeriods")" />
                Periods occur every
                <input type="text" class="form-control" name="GenitourinaryPeriodsEveryXDays" placeholder="" data-ng-model="@(modelPrefix + "GenitourinaryPeriodsEveryXDays")" />
                days and last
                <input type="text" class="form-control" name="GenitourinaryPeriodsLastDays" placeholder="" data-ng-model="@(modelPrefix + "GenitourinaryPeriodsLastDays")" />
                days
                Onset of last period
                <input type="text" class="form-control" name="GenitourinaryOnsetOfLastPeriod" placeholder="" data-ng-model="@(modelPrefix + "OnsetOfLastPeriod")" />
                @*Comment
                <input type="text" class="form-control" name="GenitourinaryComment" placeholder="" data-ng-model="@(modelPrefix + "GenitourinaryComment")" />*@
            </div>

            @DoctorReview.GetSectionFooter(modelPrefix, "Genitourinary", "Genitourinary")


            @* ----------------------------------  Lymphatic  ------------------------------ *@

            @DoctorReview.GetSectionHeader(modelPrefix, "Lymphatic", "Lymphatic/Hematologic")
