            @FWHtml.CheckBox(modelPrefix, "MuscHasRecurringBackNeckPain", "Recurring back/neck pain")
            @FWHtml.CheckBox(modelPrefix, "MuscHasBackNeckInjury", "Back/neck injury")
            @DoctorReview.GetSectionFooter(modelPrefix, "Musc", "Musculoskeletal")

            @* ----------------------------------  Neurologic  ------------------------------ *@

            @DoctorReview.GetSectionHeader(modelPrefix, "Neurologic", "Neurologic")
            @FWHtml.CheckBox(modelPrefix, "NeurologicHasSeizures", "Seizures")
            @FWHtml.CheckBox(modelPrefix, "NeurologicHasTremorsShakiness", "Tremors/shakiness")
            @FWHtml.CheckBox(modelPrefix, "NeurologicHasUnusualClumsiness", "Unusual clumsiness")
            @FWHtml.CheckBox(modelPrefix, "NeurologicHasLimbWeakness", "Limb weakness")
            @FWHtml.CheckBox(modelPrefix, "NeurologicHasNumbnessTingling", "Numbness/tingling")
            @FWHtml.CheckBox(modelPrefix, "NeurologicHasStroke", "Stroke")
            @FWHtml.CheckBox(modelPrefix, "NeurologicHasHistoryOfHeadInjury", "History of significant head injury")
            @FWHtml.CheckBox(modelPrefix, "NeurologicHasAlteredConsciousness", "Altered consciousness or black-outs")
            @DoctorReview.GetSectionFooter(modelPrefix, "Neurologic", "Neurologic")


            @* ----------------------------------  Psychologic  ------------------------------ *@

            @DoctorReview.GetSectionHeader(modelPrefix, "Psycho", "Psychologic")
            @FWHtml.CheckBox(modelPrefix, "PsychoHasLapsesInMemory", "Lapses in memory")
            @FWHtml.CheckBox(modelPrefix, "PsychoHasPeriodsOfConfusion", "Periods of confusion/disorientation")
            @FWHtml.CheckBox(modelPrefix, "PsychoHasDifficultyConcentrating", "Difficulty concentrating")
            @FWHtml.CheckBox(modelPrefix, "PsychoHasTroublesomeDepression", "Troublesome depression")
            @FWHtml.CheckBox(modelPrefix, "PsychoHasWorryAboutThings", "Worry about things")
