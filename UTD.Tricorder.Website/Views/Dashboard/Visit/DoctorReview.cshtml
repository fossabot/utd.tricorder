            @FWHtml.CheckBox(modelPrefix, "LymphaticHasBruiseEasily", "Bruise easily")
            @FWHtml.CheckBox(modelPrefix, "LymphaticHasUnusualBleeding", "Unusual bleeding")
            @DoctorReview.GetSectionFooter(modelPrefix, "Lymphatic", "Lymphatic/Hematologic")


            @* ----------------------------------  Musculoskeletal  ------------------------------ *@

