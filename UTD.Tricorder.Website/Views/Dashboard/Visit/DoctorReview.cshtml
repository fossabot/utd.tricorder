            @DoctorReview.GetSectionFooter(modelPrefix, "Psycho", "Psychologic")

            @* ----------------------------------  Skin  ------------------------------ *@

            @DoctorReview.GetSectionHeader(modelPrefix, "Skin", "Skin")
            @FWHtml.CheckBox(modelPrefix, "SkinHasItching", "Itching")
            @FWHtml.CheckBox(modelPrefix, "SkinHasRash", "Rash")
            @FWHtml.CheckBox(modelPrefix, "SkinHasUnusualDryness", "Unusual dryness")
            @FWHtml.CheckBox(modelPrefix, "SkinHasChangesInHair", "Changes in hair")
            @FWHtml.CheckBox(modelPrefix, "SkinHasChangesInPigmentation", "Changes in pigmentation")
            @DoctorReview.GetSectionFooter(modelPrefix, "Skin", "Skin")


            @* ----------------------------------  Endocrine  ------------------------------ *@

            @DoctorReview.GetSectionHeader(modelPrefix, "Endo", "Endocrine")
            Unexpected changes in:
            @FWHtml.CheckBox(modelPrefix, "EndoHasToleranceToHeat", "Tolerance to heat")
            @FWHtml.CheckBox(modelPrefix, "EndoHasToleranceToCold", "Tolerance to cold")
            @FWHtml.CheckBox(modelPrefix, "EndoHasUnusualThirst", "Unusual thirst")
            @DoctorReview.GetSectionFooter(modelPrefix, "Endo", "Endocrine")


            @* ----------------------------------  Allergy  ------------------------------ *@

            @DoctorReview.GetSectionHeader(modelPrefix, "Allergy", "Allergy/lmmunolo")
            @FWHtml.CheckBox(modelPrefix, "AllergyHasSeasonalAllergies", "Seasonal allergies")
            @FWHtml.CheckBox(modelPrefix, "AllergyHasSensitivityToItems", "Sensitivity to specific items")
            <input type="text" class="form-control" name="AllergySensitivityItems" placeholder="Sensitivity items" data-ng-model="@(modelPrefix + "AllergySensitivityItems")" />
            @FWHtml.CheckBox(modelPrefix, "AllergyHasFrequentInfections", "Frequent or unusual infections")
            @DoctorReview.GetSectionFooter(modelPrefix, "Allergy", "Allergy/lmmunolo")


